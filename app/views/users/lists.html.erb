<div class="container">
  <p id="notice"><%= notice %></p>
  <div class="title">
    <h1><%= @user.name %>のLists</h1>
  </div>
  <table class="table table-striped">
      <thead>
          <tr>
              <th>#</th>
              <th>リスト名</th>
              <th>コンテンツ</th>
              <th></th>
              <th></th>
              <th></th>
          </tr>
      </thead>
      <tbody>
        <% @lists.each do |list| %>
          <tr>
              <th><%= list.id %></th>
              <td><%= list.title %></td>
              <td><%= list.content %></td>
              <td><%= link_to 'Show', list %></td>
              <td><%= link_to 'Edit', edit_list_path(list) %></td>
              <td><%= link_to 'Destroy', list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
  </table>
</div>